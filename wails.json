{
  "$schema": "https://raw.githubusercontent.com/APshenkin/wails/feature/open-files/website/static/schemas/config.v2.json",
  "name": "wails-open-file",
  "outputfilename": "wails-open-file",
  "frontend:install": "npm install",
  "frontend:build": "npm run build",
  "frontend:dev:watcher": "npm run dev",
  "frontend:dev:serverUrl": "auto",
  "author": {
    "name": "Andrey Pshenkin",
    "email": "andrey.pshenkin@gmail.com"
  },
  "info": {
    "fileAssociations": [
      {
        "ext": "wwwails",
        "name": "Lets Test Wails",
        "description": "Lets New Wails file",
        "iconName": "fileIcon",
        "role": "Editor",
        "mimeType": "foo"
      },
      {
        "ext": "ppshenkin",
        "name": "Test Pshenkin File",
        "description": "Pshenkin file",
        "iconName": "pshenkin",
        "role": "Editor",
        "mimeType": "foo"
      }
    ]
  }
}
